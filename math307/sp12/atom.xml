---
layout: nil
base: math307/sp12/
coursetitle: Math 307
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

<title>{{page.coursetitle}}</title>
<link href="{{site.url}}/{{page.base}}/atom.xml" rel="self"/>
<link href="{{site.url}}/{{page.base}}/"/>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{site.url}}/{{page.base}}</id>

<author>
  <name>Nathan Grigg</name>
  <email>grigg@uw.edu</email>
</author>

{% for post in site.categories.sp12 %}
{% if post.categories contains "math307" %}
<entry>
{% if post.categories contains "lessons" %}
	<title>[Lesson {{ post.lesson }}] {{ post.title }}</title>
	<content type="html">
		&lt;p&gt;{{post.description | xml_escape }}&lt;/p&gt;
		&lt;p&gt;&lt;a href=&quot;{{ site.url }}{{ post.url }}&quot;&gt;View the lesson&lt;/a&gt;&lt;/p&gt;
	</content>
{% else %}
   <title>{{ post.title }}</title>
   <content type="html">{{ post.content | xml_escape }}</content>
{% endif %}

<link href="{{ site.url }}{{ post.url }}"/>
<updated>{{ post.date | date_to_xmlschema }}</updated>
<id>{{site.url}}/{{page.base}}{{ post.id }}</id>
</entry>
{% endif %}
{% endfor %}

</feed>
