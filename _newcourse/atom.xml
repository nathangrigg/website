---
layout: nil
base: math~NNN~/~QQ~~YY~/
coursetitle: Math ~NNN~
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{page.coursetitle}}</title>
 <link href="{{site.url}}{{base}}atom.xml" rel="self"/>
 <link href="{{site.url}}{{base}}"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{site.url}}{{base}}</id>

 <author>
   <name>Nathan Grigg</name>
   <email>grigg@uw.edu</email>
 </author>

 {% for post in site.categories.~QQ~~YY~ %}
 {% if post.categories contains "math~NNN~" %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ site.url }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endif %}
 {% endfor %}

</feed>
